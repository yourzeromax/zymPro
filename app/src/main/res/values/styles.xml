<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="loginActivity_dimens">
        <item name="android:layout_alignStart">@id/tv_wjmm</item>
        <item name="android:layout_alignEnd">@id/tv_yhzc</item>
    </style>

<style name="loginActivity_lines">
    <item name="android:layout_height">1dp</item>
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_centerVertical">true</item>
    <item name="android:background">@color/lineColor</item>
    <item name="android:layout_marginEnd">@dimen/line_margin_dimen</item>
    <item name="android:layout_marginStart">@dimen/line_margin_dimen</item>
</style>

</resources>
